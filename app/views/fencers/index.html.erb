

<% @sfencers = @fencers.sort_by { |f| f.name } %>
<% @sfencers.each do | fencer |%>
  <div class="small-6 medium-4 large-2 end columns media-object">
    <div class="media-object-section">
      <a href="https://s3.amazonaws.com/engardeappfencingimages/fencer<%= fencer.id %><%=".jpeg"%>"> <img class="thumbnail" src="https://s3.amazonaws.com/engardeappfencingimages/fencer<%= fencer.id %><%=".jpeg"%>" style="width: 125px; height: 175px"></a>
      <center><h5><%= link_to fencer.name, fencer_path(fencer) %></h5></center>
    </div>
  </div>
  <% end %>
  <%= link_to "Add Fencer", new_fencer_path(current_user), class: "button purple-button margin-book-info"%>
</div>
